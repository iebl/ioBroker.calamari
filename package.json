{
  "name": "iobroker.calamari",
  "version": "0.0.1",
  "description": "Octopus DE API",
  "author": {
    "name": "huepfman",
    "email": "trammer@iebl.de"
  },
  "homepage": "https://github.com/huepfman/ioBroker.calamari",
  "license": "MIT",
  "keywords": [
    "octopus",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/huepfman/ioBroker.calamari.git"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.2.3",
    "axios": "^1.9.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.2",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@iobroker/adapter-dev": "^1.4.0",
    "@iobroker/testing": "^5.0.4",
    "@release-it/conventional-changelog": "^10.0.1",
    "@tsconfig/node20": "^20.1.5",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.47",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^3.2.12",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "mocha": "^11.3.0",
    "prettier": "^3.5.3",
    "proxyquire": "^2.1.3",
    "release-it": "^19.0.2",
    "sinon": "^20.0.0",
    "sinon-chai": "^3.7.0",
    "typescript": "~5.0.4"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).{json,json5}",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint .",
    "translate": "translate-adapter",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major", 
    "build": "npm run build:ts",
    "build:ts": "tsc -p tsconfig.build.json"
  },
  "bugs": {
    "url": "https://github.com/huepfman/ioBroker.calamari/issues"
  },
  "readmeFilename": "README.md"
}